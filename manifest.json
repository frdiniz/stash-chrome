{
  "name": "Stash",
  "description": "Export my bookmarks for Jsonbin.org",
  "version": "1.1",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_title": "Export Bookmarks",
    "default_popup": "popup.html"
  },
  "permissions": [
    "unlimitedStorage",
    "bookmarks",
    "*://*.jsonbin.org/",
    "*://*.maxcdn.bootstrapcdn.com/*"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
