{
  "manifest_version": 2,
  "name": "Stashed",
  "description": "Export my bookmarks",
  "version": "1.1",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    },
    "default_title": "That's the tool tip",
    "default_popup": "popup.html"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "bookmarks"
  ]
}
